	.syntax unified
	.text
	.global main
main:	mov		r0,#0			//Initalizes r0 to 0 as a counter to move through the string
		ldr		r1,=String		//Loads address of string into r1
loop:	ldrb	r1,[r1,r0]		//Loads next character of string into r1 for analysis
		beq		end				//If loaded value is ascii code 0(null), branch to end of program
		cmp		r1,#65			//Detect captial letters comparing to the lowest ascii code
		blt		good			//If ascii is less than 65 it does not need to be converted
		cmp		r1,#90			//Detect capital letters by comparing to the highest ascii code
		bgt		good			//If ascii is greater than 90 it does not need to be converted


good:	push	{r1}
		bl		loop


end:	b		.

// Data section starts at 0x20000000
	.data
String:	.space	20		//Allocates 20 bytes to store characters of the ASCII string
